<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dongbao.api.ums.mapper.UmsMemberMapper">

    <select id="selectByUsername" resultType="com.dongbao.api.ums.entity.UmsMember" parameterType="java.lang.String" >
        select * from ums_member where username=#{username,jdbcType=VARCHAR}
    </select>

</mapper>
